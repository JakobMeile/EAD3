                <did>
                    <unittitle>Correspondence</unittitle>
                    <unitdate normal="1945-01-01/2005-12-31">1945-2005</unitdate>
                    <physdescstructured coverage="whole" physdescstructuredtype="otherphysdescstructuredtype" otherphysdescstructuredtype="container">
